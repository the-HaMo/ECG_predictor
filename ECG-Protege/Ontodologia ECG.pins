([Ontodologia+ECG_Class7] of  %3APAL-CONSTRAINT

	(%3APAL-NAME "Final mayor que el inicio")
	(%3APAL-STATEMENT "(defrange ?intervalo :FRAME INTERVALO)\n(defrange ?intervaloPQ :FRAME Intervalo_PQ)\n(defrange ?intervaloQT :FRAME Intervalo_QT)\n(defrange ?ondaP :FRAME Onda_P)\n(defrange ?ondaQ :FRAME Onda_Q)\n(defrange ?ondaT :FRAME Onda_T)\n\n; Fin > Inicio\n(forall ?intervalo\n        (=> (and (own-slot-not-null Inicio ?intervalo)\n                 (own-slot-not-null Fin ?intervalo))\n            (> (Fin ?intervalo) (Inicio ?intervalo))\n        )\n)\n\n; Duración >= 0\n(forall ?intervalo\n        (=> (own-slot-not-null Duración ?intervalo)\n            (>= (Duración ?intervalo) 0)\n        )\n)\n\n; Duración = Fin - Inicio\n(forall ?intervalo\n        (=> (and (own-slot-not-null Inicio ?intervalo)\n                 (own-slot-not-null Fin ?intervalo)\n                 (own-slot-not-null Duración ?intervalo))\n            (= (Duración ?intervalo) (- (Fin ?intervalo) (Inicio ?intervalo)))\n        )\n)\n"))

