([Ontodologia+ECG_Class11] of  %3APAL-CONSTRAINT

	(%3APAL-DESCRIPTION "Valida que el pico de amplitud de la onda sea un valor negativo")
	(%3APAL-NAME "Amplitud negativa")
	(%3APAL-RANGE "OndaQ\n")
	(%3APAL-STATEMENT "; La onda Q debe ser negativa\n(forall ?ondaQ\n        (=> (own-slot-not-null Peak ?ondaQ)\n            (< (Peak ?ondaQ) 0)\n        )\n)\n\n; End > Start\n(forall ?ondaQ\n        (=> (and (own-slot-not-null Start ?ondaQ)\n                 (own-slot-not-null End ?ondaQ))\n            (> (End ?ondaQ) (Start ?ondaQ))\n        )\n)\n\n"))

([Ontodologia+ECG_Class12] of  %3APAL-CONSTRAINT

	(%3APAL-DESCRIPTION "Valida que el pico de amplitud de la onda sea un valor positivo")
	(%3APAL-NAME "Amplitud positiva")
	(%3APAL-RANGE "OndaR")
	(%3APAL-STATEMENT "; La onda R debe ser positiva\n(forall ?ondaR\n        (=> (own-slot-not-null Peak ?ondaR)\n            (> (Peak ?ondaR) 0)\n        )\n)\n\n; End > Start\n(forall ?ondaR\n        (=> (true)\n            (> (End ?ondaR) (Start ?ondaR))\n        )\n)\n\n\n"))

